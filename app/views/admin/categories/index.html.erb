<h1>Admin::Categories#index</h1>

<div class="well">
  <%= link_to '+ New Category', [:new, :admin, :category], class: 'btn btn-info' %>
</div>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Plant Category</th>
      <th scope="col">Type Count</th>
    </tr>
  </thead>
  <tbody>
    <%= render @categories %>
  </tbody>
</table>

<p>Find me in app/views/admin/categories/index.html.erb</p>
